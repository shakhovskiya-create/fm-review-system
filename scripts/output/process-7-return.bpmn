      <bpmn:outgoing>Flow_Within60</bpmn:outgoing>
      <bpmn:outgoing>Flow_After60</bpmn:outgoing>
    </bpmn:exclusiveGateway>
    <bpmn:serviceTask id="Task_Sanctions" name="Учет в санкциях (% выкупа)">
      <bpmn:incoming>Flow_Within60</bpmn:incoming>
      <bpmn:outgoing>Flow_SanctionsEnd</bpmn:outgoing>
    </bpmn:serviceTask>
    <bpmn:serviceTask id="Task_FinCorrection" name="Только финансовая корректировка">
      <bpmn:incoming>Flow_After60</bpmn:incoming>
      <bpmn:outgoing>Flow_FinEnd</bpmn:outgoing>
    </bpmn:serviceTask>
    <bpmn:userTask id="Task_Storno" name="Сторнирование РТУ (товар не покидал склад)">
      <bpmn:outgoing>Flow_StornoToRecalc</bpmn:outgoing>
    </bpmn:userTask>
    <bpmn:endEvent id="EndEvent_OK" name="Возврат обработан">

      <bpmn:outgoing>Flow_CancelToEnd</bpmn:outgoing>
    </bpmn:serviceTask>
    <bpmn:serviceTask id="Task_ReturnClosed" name="Возврат по закрытой ЛС (товар в своб. остаток)">
      <bpmn:incoming>Flow_Closed</bpmn:incoming>
      <bpmn:outgoing>Flow_To60d</bpmn:outgoing>
    </bpmn:serviceTask>
    <bpmn:exclusiveGateway id="Gateway_60d">

        <bpmn:flowNodeRef>StartEvent_1</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_InitReturn</bpmn:flowNodeRef>
      </bpmn:lane>
      <bpmn:lane id="Lane_System" name="1С:УТ">
        <bpmn:flowNodeRef>Gateway_LS</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_CreateReturn</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_ToRemainder</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_Recalc</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Gateway_Level</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_CancelApproval</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_ReturnClosed</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Gateway_60d</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_Sanctions</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_FinCorrection</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>EndEvent_OK</bpmn:flowNodeRef>
      </bpmn:lane>
      <bpmn:lane id="Lane_Exchange" name="Специалист товарообмена">
        <bpmn:flowNodeRef>Task_Storno</bpmn:flowNodeRef>
      </bpmn:lane>
    </bpmn:laneSet>
    <bpmn:startEvent id="StartEvent_1">
      <bpmn:outgoing>Flow_1</bpmn:outgoing>
    </bpmn:startEvent>
    <bpmn:userTask id="Task_InitReturn" name="Инициация возврата">
      <bpmn:incoming>Flow_1</bpmn:incoming>
      <bpmn:outgoing>Flow_ToLS</bpmn:outgoing>
    </bpmn:userTask>
    <bpmn:exclusiveGateway id="Gateway_LS">
      <bpmn:incoming>Flow_ToLS</bpmn:incoming>
      <bpmn:outgoing>Flow_Active</bpmn:outgoing>
      <bpmn:outgoing>Flow_Closed</bpmn:outgoing>
    </bpmn:exclusiveGateway>
    <bpmn:serviceTask id="Task_CreateReturn" name="Создание Возврат товаров (привязка к РТУ)">
      <bpmn:incoming>Flow_Active</bpmn:incoming>
      <bpmn:outgoing>Flow_ToRemainder</bpmn:outgoing>
    </bpmn:serviceTask>
    <bpmn:serviceTask id="Task_ToRemainder" name="Товар в остаток ЛС (рент. по цене РТУ)">
      <bpmn:incoming>Flow_ToRemainder</bpmn:incoming>
      <bpmn:outgoing>Flow_ToRecalc</bpmn:outgoing>
    </bpmn:serviceTask>
    <bpmn:serviceTask id="Task_Recalc" name="Пересчет накопленной рентабельности">
      <bpmn:incoming>Flow_ToRecalc</bpmn:incoming>

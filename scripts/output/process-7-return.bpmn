    <bpmn:sequenceFlow id="Flow_LevelChanged" name="уровень изменился" sourceRef="Gateway_Level" targetRef="Task_CancelApproval" />
    <bpmn:sequenceFlow id="Flow_NoChange" name="без изменений" sourceRef="Gateway_Level" targetRef="EndEvent_OK" />
    <bpmn:sequenceFlow id="Flow_CancelToEnd" sourceRef="Task_CancelApproval" targetRef="EndEvent_OK" />
    <bpmn:sequenceFlow id="Flow_To60d" sourceRef="Task_ReturnClosed" targetRef="Gateway_60d" />
    <bpmn:sequenceFlow id="Flow_Within60" name="&#8804; 60 дней" sourceRef="Gateway_60d" targetRef="Task_Sanctions" />
    <bpmn:sequenceFlow id="Flow_After60" name="&gt; 60 дней" sourceRef="Gateway_60d" targetRef="Task_FinCorrection" />
    <bpmn:sequenceFlow id="Flow_SanctionsEnd" sourceRef="Task_Sanctions" targetRef="EndEvent_OK" />
    <bpmn:sequenceFlow id="Flow_FinEnd" sourceRef="Task_FinCorrection" targetRef="EndEvent_OK" />

      <bpmn:outgoing>Flow_LevelChanged</bpmn:outgoing>
      <bpmn:outgoing>Flow_NoChange</bpmn:outgoing>
    </bpmn:exclusiveGateway>
    <bpmn:serviceTask id="Task_CancelApproval" name="Аннулирование согласований">

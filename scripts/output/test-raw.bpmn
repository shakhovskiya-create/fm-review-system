<?xml version="1.0" encoding="UTF-8"?>
<bpmn2:definitions xmlns:bpmn2="http://www.omg.org/spec/BPMN/20100524/MODEL" xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI" xmlns:dc="http://www.omg.org/spec/DD/20100524/DC" xmlns:di="http://www.omg.org/spec/DD/20100524/DI" id="Definitions_1" targetNamespace="http://bpmn.io/schema/bpmn">
  <bpmn2:process id="Process_1" isExecutable="false">
    <bpmn2:startEvent id="StartEvent_1" name="Старт">
      <bpmn2:outgoing>Flow_1</bpmn2:outgoing>
    </bpmn2:startEvent>
    <bpmn2:userTask id="Task_1" name="Создать заказ клиента">
      <bpmn2:incoming>Flow_1</bpmn2:incoming>
      <bpmn2:outgoing>Flow_2</bpmn2:outgoing>
    </bpmn2:userTask>
    <bpmn2:serviceTask id="Task_2" name="Проверить НПСС">
      <bpmn2:incoming>Flow_2</bpmn2:incoming>
      <bpmn2:outgoing>Flow_3</bpmn2:outgoing>
    </bpmn2:serviceTask>
    <bpmn2:exclusiveGateway id="Gateway_1" name="НПСС актуальна?">
      <bpmn2:incoming>Flow_3</bpmn2:incoming>
      <bpmn2:outgoing>Flow_4</bpmn2:outgoing>
      <bpmn2:outgoing>Flow_5</bpmn2:outgoing>
    </bpmn2:exclusiveGateway>
    <bpmn2:userTask id="Task_3" name="Заблокировать заказ">
      <bpmn2:incoming>Flow_4</bpmn2:incoming>
      <bpmn2:outgoing>Flow_6</bpmn2:outgoing>
    </bpmn2:userTask>
    <bpmn2:endEvent id="EndEvent_1" name="Конец (заблокирован)">
      <bpmn2:incoming>Flow_6</bpmn2:incoming>
    </bpmn2:endEvent>
    <bpmn2:serviceTask id="Task_4" name="Рассчитать рентабельность">
      <bpmn2:incoming>Flow_5</bpmn2:incoming>
      <bpmn2:outgoing>Flow_7</bpmn2:outgoing>
    </bpmn2:serviceTask>
    <bpmn2:exclusiveGateway id="Gateway_2" name="Рентабельность >= 0%?">
      <bpmn2:incoming>Flow_7</bpmn2:incoming>
      <bpmn2:outgoing>Flow_8</bpmn2:outgoing>
      <bpmn2:outgoing>Flow_9</bpmn2:outgoing>
    </bpmn2:exclusiveGateway>
    <bpmn2:serviceTask id="Task_5" name="Автосогласование">
      <bpmn2:incoming>Flow_8</bpmn2:incoming>
      <bpmn2:outgoing>Flow_10</bpmn2:outgoing>
    </bpmn2:serviceTask>
    <bpmn2:endEvent id="EndEvent_2" name="Конец (согласовано)">
      <bpmn2:incoming>Flow_10</bpmn2:incoming>
      <bpmn2:incoming>Flow_13</bpmn2:incoming>
    </bpmn2:endEvent>
    <bpmn2:userTask id="Task_6" name="Согласование">
      <bpmn2:incoming>Flow_9</bpmn2:incoming>
      <bpmn2:outgoing>Flow_11</bpmn2:outgoing>
    </bpmn2:userTask>
    <bpmn2:exclusiveGateway id="Gateway_3" name="Решение?">
      <bpmn2:incoming>Flow_11</bpmn2:incoming>
      <bpmn2:outgoing>Flow_12</bpmn2:outgoing>
      <bpmn2:outgoing>Flow_13</bpmn2:outgoing>
    </bpmn2:exclusiveGateway>
    <bpmn2:endEvent id="EndEvent_3" name="Конец (отклонено)">
      <bpmn2:incoming>Flow_12</bpmn2:incoming>
    </bpmn2:endEvent>
    <bpmn2:sequenceFlow id="Flow_1" sourceRef="StartEvent_1" targetRef="Task_1"/>
    <bpmn2:sequenceFlow id="Flow_2" sourceRef="Task_1" targetRef="Task_2"/>
    <bpmn2:sequenceFlow id="Flow_3" sourceRef="Task_2" targetRef="Gateway_1"/>
    <bpmn2:sequenceFlow id="Flow_4" name="Нет" sourceRef="Gateway_1" targetRef="Task_3"/>
    <bpmn2:sequenceFlow id="Flow_5" name="Да" sourceRef="Gateway_1" targetRef="Task_4"/>
    <bpmn2:sequenceFlow id="Flow_6" sourceRef="Task_3" targetRef="EndEvent_1"/>
    <bpmn2:sequenceFlow id="Flow_7" sourceRef="Task_4" targetRef="Gateway_2"/>
    <bpmn2:sequenceFlow id="Flow_8" name="Да" sourceRef="Gateway_2" targetRef="Task_5"/>
    <bpmn2:sequenceFlow id="Flow_9" name="Нет" sourceRef="Gateway_2" targetRef="Task_6"/>
    <bpmn2:sequenceFlow id="Flow_10" sourceRef="Task_5" targetRef="EndEvent_2"/>
    <bpmn2:sequenceFlow id="Flow_11" sourceRef="Task_6" targetRef="Gateway_3"/>
    <bpmn2:sequenceFlow id="Flow_12" name="Отклонено" sourceRef="Gateway_3" targetRef="EndEvent_3"/>
    <bpmn2:sequenceFlow id="Flow_13" name="Согласовано" sourceRef="Gateway_3" targetRef="EndEvent_2"/>

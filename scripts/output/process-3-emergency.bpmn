  <bpmn:collaboration id="Collaboration_1">
    <bpmn:participant id="Participant_Pool" name="Экстренное согласование (устное разрешение)" processRef="Process_Emergency" />
  </bpmn:collaboration>
  <bpmn:process id="Process_Emergency" processType="None" isClosed="false" isExecutable="true">
    <bpmn:laneSet id="LaneSet_1">
      <bpmn:lane id="Lane_Manager" name="Менеджер">
        <bpmn:flowNodeRef>StartEvent_1</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_RequestOral</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Gateway_OralResult</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_Fix1C</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>CallActivity_Standard</bpmn:flowNodeRef>
      </bpmn:lane>
      <bpmn:lane id="Lane_Warehouse" name="Склад">
        <bpmn:flowNodeRef>Task_Ship</bpmn:flowNodeRef>
      </bpmn:lane>
      <bpmn:lane id="Lane_Approver" name="Согласующий">
        <bpmn:flowNodeRef>Task_PostFactum</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Gateway_PostResult</bpmn:flowNodeRef>
      </bpmn:lane>
      <bpmn:lane id="Lane_Result" name="Результат">
        <bpmn:flowNodeRef>Task_Incident</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>EndEvent_Incident</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>EndEvent_Approved</bpmn:flowNodeRef>
      </bpmn:lane>
    </bpmn:laneSet>
    <bpmn:startEvent id="StartEvent_1" name="Срочная отгрузка">
      <bpmn:outgoing>Flow_1</bpmn:outgoing>
    </bpmn:startEvent>
    <bpmn:userTask id="Task_RequestOral" name="Запрос устного разрешения">
      <bpmn:incoming>Flow_1</bpmn:incoming>
      <bpmn:outgoing>Flow_2</bpmn:outgoing>
    </bpmn:userTask>
    <bpmn:exclusiveGateway id="Gateway_OralResult">
      <bpmn:incoming>Flow_2</bpmn:incoming>
      <bpmn:outgoing>Flow_Yes</bpmn:outgoing>
      <bpmn:outgoing>Flow_No</bpmn:outgoing>
    </bpmn:exclusiveGateway>
    <bpmn:userTask id="Task_PostFactum" name="Согласование пост-фактум (SLA 24ч)">
      <bpmn:incoming>Flow_ToPostFactum</bpmn:incoming>
      <bpmn:outgoing>Flow_PostResult</bpmn:outgoing>
    </bpmn:userTask>
    <bpmn:exclusiveGateway id="Gateway_PostResult">
      <bpmn:incoming>Flow_PostResult</bpmn:incoming>
      <bpmn:outgoing>Flow_PostYes</bpmn:outgoing>
      <bpmn:outgoing>Flow_PostNo</bpmn:outgoing>
    </bpmn:exclusiveGateway>
    <bpmn:sequenceFlow id="Flow_1" sourceRef="StartEvent_1" targetRef="Task_RequestOral" />
    <bpmn:sequenceFlow id="Flow_2" sourceRef="Task_RequestOral" targetRef="Gateway_OralResult" />
    <bpmn:sequenceFlow id="Flow_Yes" name="да" sourceRef="Gateway_OralResult" targetRef="Task_Fix1C">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">${oralApproval == true}</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    <bpmn:sequenceFlow id="Flow_No" name="нет" sourceRef="Gateway_OralResult" targetRef="CallActivity_Standard">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">${oralApproval == false}</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    <bpmn:sequenceFlow id="Flow_ToShip" sourceRef="Task_Fix1C" targetRef="Task_Ship" />
    <bpmn:sequenceFlow id="Flow_ToPostFactum" sourceRef="Task_Ship" targetRef="Task_PostFactum" />
    <bpmn:sequenceFlow id="Flow_PostResult" sourceRef="Task_PostFactum" targetRef="Gateway_PostResult" />
    <bpmn:sequenceFlow id="Flow_PostYes" name="да" sourceRef="Gateway_PostResult" targetRef="EndEvent_Approved">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">${postApproved == true}</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    <bpmn:sequenceFlow id="Flow_PostNo" name="нет" sourceRef="Gateway_PostResult" targetRef="Task_Incident">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">${postApproved == false}</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    <bpmn:sequenceFlow id="Flow_IncidentEnd" sourceRef="Task_Incident" targetRef="EndEvent_Incident" />
    <bpmn:userTask id="Task_Fix1C" name="Фиксация в 1С (комментарий)">
      <bpmn:incoming>Flow_Yes</bpmn:incoming>
      <bpmn:outgoing>Flow_ToShip</bpmn:outgoing>
    </bpmn:userTask>
    <bpmn:serviceTask id="Task_Ship" name="Отгрузка товара">
      <bpmn:incoming>Flow_ToShip</bpmn:incoming>
      <bpmn:outgoing>Flow_ToPostFactum</bpmn:outgoing>
    </bpmn:serviceTask>
    <bpmn:subProcess id="CallActivity_Standard" name="Стандартный процесс (см. BPMN 2)">
      <bpmn:incoming>Flow_No</bpmn:incoming>
    </bpmn:subProcess>
    <bpmn:serviceTask id="Task_Incident" name="Регистрация инцидента">
      <bpmn:incoming>Flow_PostNo</bpmn:incoming>
      <bpmn:outgoing>Flow_IncidentEnd</bpmn:outgoing>
    </bpmn:serviceTask>
    <bpmn:endEvent id="EndEvent_Incident" name="Инцидент">
      <bpmn:incoming>Flow_IncidentEnd</bpmn:incoming>
    </bpmn:endEvent>
    <bpmn:endEvent id="EndEvent_Approved" name="Согласовано пост-фактум">
      <bpmn:incoming>Flow_PostYes</bpmn:incoming>
    </bpmn:endEvent>
  </bpmn:process>
